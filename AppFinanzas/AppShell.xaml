<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppFinanzas.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:AppFinanzas.Mvvm.Views"
    
    xmlns:local="clr-namespace:AppFinanzas"
    Shell.FlyoutBehavior="Disabled"
    Title="AppFinanzas">



    <ShellContent Route="LoginPage" Title="Home" ContentTemplate="{DataTemplate views:LoginPage}"   />
    <ShellContent Route="MenuPage" ContentTemplate="{DataTemplate views:MenuPage}" />
    <ShellContent Route="CuentasPage" ContentTemplate="{DataTemplate views:CuentasPage}" />
    <ShellContent Route="NuevaTransaccionPage" ContentTemplate="{DataTemplate views:NuevaTransaccionPage}" />



</Shell>

